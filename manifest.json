{
  "manifest_version": 2,

  "name": "Ocubrowse",
  "version": "0.1.0",
  "description": "Browse the web hands-free with a chrome extension",
  "author": "Real cool BU students",

  "browser_action": {
    "default_title": "Default title",
    "default_popup": "html/popup.html"
  },

  "content_scripts": [{
    "css": ["styles/styles.css"],
    "js": ["js/content.js"],
    "matches": ["<all_urls>"]
  }],

  "permissions": ["activeTab"]
}
